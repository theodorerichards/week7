//import processing.pdf.*;
PImage img;

void setup() {
  size(600, 372);
  //size(600,372,PDF, "code4.pdf");
  img = loadImage("bust.jpg");
}

void draw() {
  background(#8DD7ED);
  for (int i = 0; i < width; i += 10) {
    for (int j = 0; j < height; j += 10) {
      color c = img.get(i, j);
      stroke(c);
      
      line(i,j,width/2,height/2);
      fill(c);
      ellipse(i+20,j+20, 5,5);
    }
}
//println("Finished.");
//  exit();
}
